{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Sentry": {
    "Dsn": "",
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "Debug": false,
    "DiagnosticsLevel": "Error"
  },
  "Urls": "http://*:5000",
  "AllowedHosts": "*",
  "PostsStreamer": {
    "MaxDegreeOfParallelism": 4
  },
  "Scraper": {
    "Feeds": {
      "Enabled": true
    },
    "Twitter": {
      "Enabled": true,
      "ConsumerKey": "",
      "ConsumerSecret": ""
    },
    "Facebook": {
      "Enabled": true
    },
    "YoutubeDl": {
      "Enabled": false,
      "KeepReceivedPost": false,
      "YoutubeDlPath": "youtube-dl"
    },
    "Screenshot": {
      "Enabled": false,
      "KeepReceivedPost": true,
      "UserId": "",
      "ApiKey": ""
    }
  },
  "RabbitMq": {
    "Enabled": true,
    "ConnectionString": "amqp://guest:guest@rabbitmq:5672//"
  },
  "MongoDb": {
    "Enabled": true,
    "ConnectionString": "mongodb://mongodb:27017",
    "DatabaseName": "ScraperDb",
    "PostUrls": {
      "ExpirationTime": "1:00:00:00"
    }
  }
}
